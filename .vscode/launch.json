{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run ts-node-script",
            "runtimeExecutable": "ts-node-script",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug topKFrequent Test (one-click)",
            "runtimeExecutable": "node",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
            "runtimeArgs": [
                "--inspect-brk"
            ],
            "args": [
                "--runInBand",
                "src/solutions/arrays_and_hashmaps/topKFrequent /topKFrequent.test.ts"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
        ,
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Jest - Active File",
            "runtimeExecutable": "node",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
            "runtimeArgs": [
                "--inspect-brk"
            ],
            "args": [
                "--runInBand",
                "${file}"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}